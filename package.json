{
  "name": "zpass-sdk",
  "version": "0.0.1",
  "description": "zPass is a fast, secure, and privacy-preserving digital identity solution",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleoNet/zPass-SDK-new.git"
  },
  "keywords": [
    "digital identity",
    "crypto",
    "blockchain",
    "zero knowledge",
    "privacy",
    "zpass",
    "aleo",
    "zk"
  ],
  "author": "Aleo Network Foundation",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AleoNet/zPass-SDK-new/issues"
